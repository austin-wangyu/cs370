
$(document).on('turbolinks:load', function() {
  updateUserType();
});

function updateUserType(){
  var user_type = $("#user_type")[0].value;
  var form_element = $("#form_information")
  $.get({
    url: "/welcome/get_login_form",
    data: {user_type},
    success: function(data){
      console.log("success");
      form_element.empty();
      form_element.append(data['partial']);
    }
  });
}
